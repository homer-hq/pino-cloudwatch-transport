{
  "name": "@homer-hq/pino-cloudwatch-transport",
  "version": "1.1.9",
  "description": "Pino v7+ AWS CloudWatch Logs transport",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist/index.*"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "scripts": {
    "test": "vitest --ui",
    "build": "rollup --config rollup.config.esm.js && rollup --config rollup.config.cjs.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/homer-hq/pino-cloudwatch-transport.git"
  },
  "keywords": [
    "pino",
    "transport",
    "logging",
    "cloudwatch",
    "aws"
  ],
  "author": "Homer HQ - @homer-hq",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/serdnam/pino-cloudwatch-transport/issues"
  },
  "homepage": "https://github.com/serdnam/pino-cloudwatch-transport#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.12.7",
    "@vitest/ui": "^1.5.3",
    "dotenv": "^16.4.5",
    "esbuild": "^0.20.2",
    "pino": "^9.0.0",
    "p-retry": "^6.2.0",
    "rollup": "^4.17.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "tslib": "^2.6.2",
    "vitest": "^1.5.3"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.565.0",
    "async-mutex": "^0.5.0",
    "pino-abstract-transport": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "pino-abstract-transport": "^1.2.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.565.0"
  }
}